    <div class="tabs product-tabs">
        <ul id="product-infos-tabs" class="nav nav-tabs">
                            <li class="nav-item colibriDescription">
                    <a class="nav-link active" data-toggle="tab"
                       href="#description">
                        Description
                    </a>
                </li>
                        <li class="colibriDetails nav-item">
